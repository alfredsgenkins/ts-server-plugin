{
    "name": "@scandiweb/test-ts-package",
    "description": "Google Tag Manager frontend compatibility for ScandiPWA",
    "version": "2.0.0",
    "license": "UNLICENSED",
    "scandipwa": {
        "type": "extension"
    },
    "dependencies": {
        "eslint-plugin-header": "^3.1.1",
        "react-gtm-module": "2.0.11"
    },
    "eslintConfig": {
        "extends": "@scandipwa",
        "plugins": [
            "react",
            "header"
        ],
        "rules": {
            "max-lines": "off",
            "header/header": [
                2,
                "block",
                [
                    "*",
                    " * Google Tag Manager frontend compatibility for ScandiPWA",
                    " * @copyright Scandiweb, Inc. All rights reserved.",
                    " "
                ],
                2
            ]
        }
    }
}
