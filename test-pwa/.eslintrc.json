{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@scandipwa"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [
        "node_modules/**/*"
    ],
    "rules": {
        "max-lines": "off"
    }
}
